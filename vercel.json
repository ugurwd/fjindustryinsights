{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "TZ": "Europe/Istanbul"
  },
  "functions": {
    "index.js": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/trigger",
      "schedule": "0 6 * * *"
    }
  ]
}